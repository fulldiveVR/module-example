{
  "id": "simple-docs-with-chat",
  "name": "Contract Analysis Agent",
  "version": "1.2.0",
  "description": "An intelligent agent that analyzes legal contracts and identifies potential risks, compliance issues, and key terms.",
  "author": "AIWize Legal Team",
  "permissions": {
    "fileSystem": true,
    "apiProxy": true,
    "llm": true,
    "database": true,
    "network": true
  },
  "metadata": {
    "category": "legal",
    "tags": [
      "contracts",
      "compliance",
      "risk-analysis"
    ],
    "supportedLanguages": [
      "en",
      "es",
      "fr"
    ],
    "maxFileSize": "10MB",
    "supportedFormats": [
      "pdf",
      "docx",
      "txt"
    ],
    "lastUpdated": "2024-01-15T10:30:00Z",
    "dependencies": {
      "openai": "^4.0.0",
      "pdf-parse": "^1.1.1"
    },
    "config": {
      "defaultModel": "gpt-4",
      "temperature": 0.3,
      "maxTokens": 4000
    }
  }
}
