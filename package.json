{
  "name": "aiwize-combiner",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:left\" \"npm run dev:right\"",
    "dev:left": "webpack serve --config webpack.config.js --env panel=left",
    "dev:right": "webpack serve --config webpack.config.js --env panel=right",
    "build": "npm run build:left && npm run build:right",
    "build:left": "cross-env NODE_ENV=production webpack --config webpack.config.js --env panel=left --mode production",
    "build:right": "cross-env NODE_ENV=production webpack --config webpack.config.js --env panel=right --mode production",
    "build:prod": "npm run build",
    "build:dev": "npm run build:left:dev && npm run build:right:dev",
    "build:left:dev": "webpack --config webpack.config.js --env panel=left --mode development",
    "build:right:dev": "webpack --config webpack.config.js --env panel=right --mode development",
    "start": "npm run build"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "axios": "^1.6.2",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.20.1",
    "redux": "^5.0.0",
    "styled-components": "^6.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.6.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "license": "MIT"
}
